Mandala n = Create Mandala 

Foreach i = 7.0 To 10.0:
  Number x = i * 10.0
  Shape shape1 = Create Shape:
    Geo square
    Size x
    Color cyan 
    Rotation 45.0
  Layer layer1 = Create Layer:
    Radius x 
    Shape shape1 
    Count 12 
    Offset 0.0 
    AngularShift 1
  addTo:(n, layer1)
draw:(n)