Mandala m = Create Mandala

Shape s = Create Shape:
  Geo circle
  Size 10.0 
  Color black
  Rotation 0.0

Layer l = Create Layer:
  Radius 40.0
  Shape s
  Count 8
  Offset 0.0
  AngularShift 0 

Layer l2 = Create Layer:
  Radius 80.0 * 2.0
  Shape s
  Count 8
  Offset 0.0
  AngularShift 0 

Layer l3 = Create Layer:
  Radius 80.0 * 2.0 + 40.0 
  Shape s
  Count 8
  Offset 0.0
  AngularShift 0 

addTo: (m, l, l2, l3)

draw: (m) 