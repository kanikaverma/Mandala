Mandala m = Create Mandala

Shape s = Create Shape:
  Geo square
  Size 25.0
  Color black
  Rotation 0.0

Shape s2 = Create Shape:
  Geo circle
  Size 25.0
  Color black
  Rotation 0.0

Layer l = Create Layer:
  Radius 80.0
  Shape s
  Count 4
  Offset 45.0
  AngularShift 0 

Layer l2 = Create Layer:
  Radius 80.0
  Shape s2
  Count 4
  Offset 0.0
  AngularShift 0 

addTo: (m, l, l2)

draw: (m) 